{
  "name": "@jupyterlab/google-drive",
  "version": "2.0.0",
  "description": "Cloud storage with JupyterLab through Google Drive",
  "keywords": [
    "google drive",
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/jupyterlab/jupyterlab-google-drive",
  "bugs": {
    "url": "https://github.com/jupyterlab/jupyterlab-google-drive/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jupyterlab/jupyterlab-google-drive.git"
  },
  "license": "BSD-3-Clause",
  "author": "Ian Rose",
  "files": [
    "lib/*/*d.ts",
    "lib/*/*.js",
    "lib/*.d.ts",
    "lib/*.js",
    "schema/*.json",
    "style/*.*"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib/"
  },
  "scripts": {
    "build": "jlpm run build:lib && jlpm run build:labextension:dev",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "build:lib": "tsc",
    "build:prod": "jlpm run clean && jlpm run build:lib && jlpm run build:labextension",
    "build:test": "cd test && ./build-tests.sh",
    "clean": "jlpm run clean:lib",
    "clean:all": "jlpm run clean:lib && jlpm run clean:labextension",
    "clean:labextension": "rimraf jupyterlab_google_drive/labextension",
    "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
    "install:extension": "jlpm run build",
    "precommit": "lint-staged",
    "prettier": "prettier --write '**/*{.ts,.tsx,.js,.jsx,.css,.json,.md}'",
    "test": "cd test && ./run-tests.sh",
    "watch": "run-p watch:src watch:labextension",
    "watch:labextension": "jupyter labextension watch .",
    "watch:src": "tsc -w"
  },
  "lint-staged": {
    "**/*{.ts,.tsx,.css,.json,.md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@jupyterlab/application": "^3.0.0",
    "@jupyterlab/apputils": "^3.0.0",
    "@jupyterlab/coreutils": "^5.0.0",
    "@jupyterlab/docmanager": "^3.0.0",
    "@jupyterlab/docregistry": "^3.0.0",
    "@jupyterlab/filebrowser": "^3.0.0",
    "@jupyterlab/mainmenu": "^3.0.0",
    "@jupyterlab/observables": "^4.0.0",
    "@jupyterlab/services": "^6.0.0",
    "@jupyterlab/settingregistry": "^3.0.0",
    "@lumino/algorithm": "^1.3.3",
    "@lumino/commands": "^1.12.0",
    "@lumino/coreutils": "^1.5.3",
    "@lumino/signaling": "^1.4.3",
    "@lumino/widgets": "^1.16.1",
    "svg-url-loader": "^7.1.1"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^3.1.0",
    "@types/expect.js": "^0.3.29",
    "@types/gapi": "^0.0.36",
    "@types/gapi.auth2": "^0.0.50",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.0.10",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "css-loader": "^3.0.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "expect.js": "^0.3.1",
    "file-loader": "^4.0.0",
    "husky": "^2.4.1",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.1.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sourcemap-loader": "^0.3.7",
    "lint-staged": "^8.2.1",
    "mocha": "^6.1.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.1",
    "rimraf": "^3.0.2",
    "style-loader": "^0.23.1",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "~4.1.3",
    "url-loader": "^2.0.0",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.11"
  },
  "jupyterlab": {
    "extension": true,
    "schemaDir": "schema",
    "outputDir": "jupyterlab_google_drive/labextension"
  }
}
